{
    "name": "struzik-vladislav/epp-monolog-formatter",
    "description": "Monolog formatter for EPP requests/responses",
    "keywords": ["epp", "monolog", "log", "formatter"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vladislav Struzik",
            "email": "vladislav@struzik.blog"
        }
    ],
    "require": {
        "php": ">=8.1",
        "monolog/monolog": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Struzik\\EPPMonolog\\Formatter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Struzik\\EPPMonolog\\Formatter\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests/"
    }
}
